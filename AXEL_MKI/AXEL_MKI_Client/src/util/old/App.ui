            <property name="styleSheet">
             <string notr="true">QSlider {
    min-height: 20px;
    min-width: 120px;
}

/* --- Groove / Bahn --- */
QSlider::groove:horizontal {
    height: 6px;
    background: #E6EBF1;
    border-radius: 3px;
}
QSlider::groove:vertical {
    width: 6px;
    background: #E6EBF1;
    border-radius: 3px;
}

/* --- Gef√ºllter Teil (links/unten vom Griff) --- */
QSlider::sub-page:horizontal {
    background: #2E86C1;
    border-radius: 3px;
}
QSlider::sub-page:vertical {
    background: #2E86C1;
    border-radius: 3px;
}

/* Hover-Effekt */
QSlider::sub-page:horizontal:hover,
QSlider::sub-page:vertical:hover {
    background: #5DADE2;
}

/* --- Restlicher Teil (rechts/oben vom Griff) --- */
QSlider::add-page:horizontal,
QSlider::add-page:vertical {
    background: #DCE3EA;
    border-radius: 3px;
}

/* --- Griff / Handle --- */
QSlider::handle:horizontal {
    background: #FFFFFF;
    border: 2px solid #21618C;
    width: 16px;
    height: 16px;
    margin: -5px 0px;           /* Nur vertikal ausrichten, KEIN horizontaler Abstand! */
    border-radius: 8px;
}
QSlider::handle:vertical {
    background: #FFFFFF;
    border: 2px solid #21618C;
    width: 16px;
    height: 16px;
    margin: 0px -5px;           /* Nur horizontal ausrichten, KEIN vertikaler Abstand! */
    border-radius: 8px;
}

/* Hover &amp; Pressed */
QSlider::handle:horizontal:hover,
QSlider::handle:vertical:hover {
    background: #F5FAFF;
    border-color: #2E86C1;
}
QSlider::handle:horizontal:pressed,
QSlider::handle:vertical:pressed {
    background: #2E86C1;
    border-color: #1B4F72;
}
</string>
